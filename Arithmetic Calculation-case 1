import java.util.Scanner;

public class SimpleCalculator {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        
        int num1 = scanner.nextInt();
        int num2 = scanner.nextInt();
        char operator = scanner.next().charAt(0);
        scanner.close();

     switch (operator) {
            case '+':
                System.out.println("Addition of two number is " + (double)(num1 + num2));
                break;
            case '-':
                System.out.println("Subtraction of two number is " + (double)(num1 - num2));
                break;
            case '*':
                System.out.println("Multiplication of two number is " + (double)(num1 * num2));
                break;
            case '/':
                if (num2 == 0) {
                    System.out.println("Cannot divide by zero");
                } else {
                    System.out.println("Division of two number is " + (double)num1 / num2);
                }
                break;
            case '%':
                if (num2 == 0) {
                    System.out.println("Cannot perform modulo by zero");
                } else {
                    System.out.println("Modulo of two number is " + (double)(num1 % num2));
                }
                break;
            default:
                System.out.println("Invalid Input");
        }
    }
}
